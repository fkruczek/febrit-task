{
  "extends": [
    "react-app",
    "react-app/jest",
    "airbnb-typescript",
    "plugin:jsx-a11y/recommended",
    "prettier"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "plugins": ["jsx-a11y", "prettier"],
  "rules": {
    "semi": 0,
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "import/prefer-default-export": "off",
    "prettier/prettier": [
      "error",
      {
        "semi": false
      }
    ]
  }
}
